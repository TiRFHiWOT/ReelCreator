{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/concept1-minimalist.html",
      "dest": "/concept1-minimalist.html"
    },
    {
      "src": "/concept2-tiktok.html",
      "dest": "/concept2-tiktok.html"
    }
  ]
}

